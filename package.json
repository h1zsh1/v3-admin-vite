{
  "name": "ioc_vue_template",
  "version": "3.0.0",
  "description": "基于开源v3-admin-vite的管理后台模板，使用Vue3、TypeScript、Element-Plus、Pinia 和 Vite 等主流技术.",
  "author": {
    "name": "ioc"
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "scripts": {
    "start": "vite",
    "build:dev": "vue-tsc --noEmit && vite build --mode developing",
    "build:test": "vue-tsc --noEmit && vite build --mode production",
    "build:stage": "vue-tsc --noEmit && vite build --mode staging",
    "preview:stage": "pnpm build:stage && vite preview",
    "preview:prod": "pnpm build:prod && vite preview",
    "lint:eslint": "eslint \"src/**/*.{vue,ts,tsx}\" --fix",
    "lint:prettier": "prettier --write  \"src/**/*.{js,json,tsx,css,less,scss,vue,html,md}\"",
    "lint": "pnpm lint:eslint && pnpm lint:prettier",
    "prepare": "husky install"
  },
  "dependencies": {
    "@element-plus/icons-vue": "^1.1.4",
    "axios": "^0.27.2",
    "dayjs": "^1.11.1",
    "element-plus": "^2.1.11",
    "js-cookie": "^3.0.1",
    "lodash-es": "^4.17.21",
    "normalize.css": "^8.0.1",
    "nprogress": "^0.2.0",
    "path-browserify": "^1.0.1",
    "path-to-regexp": "^6.2.0",
    "pinia": "^2.0.13",
    "screenfull": "^6.0.1",
    "vue": "^3.2.33",
    "vue-router": "^4.0.15"
  },
  "devDependencies": {
    "@types/js-cookie": "^3.0.2",
    "@types/lodash-es": "^4.17.6",
    "@types/node": "^17.0.31",
    "@types/nprogress": "^0.2.0",
    "@types/path-browserify": "^1.0.0",
    "@typescript-eslint/eslint-plugin": "^5.22.0",
    "@typescript-eslint/parser": "^5.22.0",
    "@vitejs/plugin-vue": "^2.3.2",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/eslint-config-typescript": "^10.0.0",
    "eslint": "^8.14.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-vue": "^8.7.1",
    "husky": "^7.0.4",
    "lint-staged": "^12.4.1",
    "prettier": "^2.6.2",
    "sass": "^1.51.0",
    "typescript": "^4.6.4",
    "vite": "^2.9.8",
    "vite-plugin-svg-icons": "^2.0.1",
    "vue-eslint-parser": "^8.3.0",
    "vue-tsc": "^0.34.11"
  },
  "lint-staged": {
    "*.{js,jsx,vue,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{scss,less,css,html,md}": [
      "prettier --write"
    ],
    "package.json": [
      "prettier --write"
    ],
    "{!(package)*.json,.!(browserslist)*rc}": [
      "prettier --write--parser json"
    ]
  },
  "keywords": [
    "vue",
    "element-plus",
    "vue3",
    "ts",
    "admin",
    "typescript"
  ],
  "license": "MIT"
}
